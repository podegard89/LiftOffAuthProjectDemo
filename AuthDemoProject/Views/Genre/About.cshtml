@model List<AuthDemoProject.Models.SongGenre>


@if (Model.Count == 0)

{

    <h1>No songs with the given genre!</h1>

}

else

{

    <h1>Songs in this Genre: @Model[0].Genre.Name</h1>


    <ul>

        @foreach (var songGenre in Model)

        {

            <li>@songGenre.Song.Name</li>

        }

    </ul>


}


